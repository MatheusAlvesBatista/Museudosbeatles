<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDEBEB"
    tools:context=".tela4">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="219dp"
        android:layout_height="178dp"
        android:layout_marginStart="9dp"
        android:layout_marginLeft="9dp"
        android:layout_marginBottom="59dp"
        android:text="Para continuar, responda: O álbum Rubber Soul ficou marcado pelo seu estilo único, ele é considerado o álbum da:"
        android:textColor="#070707"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/buttonErva"
        app:layout_constraintStart_toEndOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="219dp"
        android:layout_height="178dp"
        android:layout_marginEnd="6dp"
        android:layout_marginRight="6dp"
        android:layout_marginBottom="12dp"
        android:text="1965 foram lançados mais dois álbuns e 1 filme, 1965 é o começo da mudança do estilo musical da banda."
        android:textColor="#070707"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="161dp"
        android:layout_height="315dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        app:layout_constraintBottom_toTopOf="@+id/buttonmaluquice"
        app:layout_constraintEnd_toStartOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/rubber" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="97dp"
        android:layout_height="51dp"
        android:layout_marginTop="20dp"
        android:text="1965"
        android:textColor="#000000"
        android:textSize="42sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableRow" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="160dp"
        android:layout_height="237dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView8"
        app:layout_constraintEnd_toStartOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/help" />

    <TableRow
        android:id="@+id/tableRow"
        android:layout_width="440dp"
        android:layout_height="74dp"
        android:layout_marginBottom="20dp"
        android:background="#0B0B0B"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonErva"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="69dp"
        android:layout_marginRight="69dp"
        android:onClick="clickTela5"
        android:text="Erva"
        app:layout_constraintBottom_toBottomOf="@+id/buttonmaluquice"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/buttonmaluquice" />

    <Button
        android:id="@+id/buttonmaluquice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="66dp"
        android:layout_marginLeft="66dp"
        android:layout_marginBottom="30dp"
        android:onClick="clickErro5"
        android:text="Maluquice"
        app:layout_constraintBottom_toTopOf="@+id/buttondist"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttondist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="clickErro5"
        android:text="Distorção"
        app:layout_constraintBottom_toBottomOf="@+id/buttonacido"
        app:layout_constraintEnd_toEndOf="@+id/buttonmaluquice"
        app:layout_constraintStart_toStartOf="@+id/buttonmaluquice"
        app:layout_constraintTop_toTopOf="@+id/buttonacido" />

    <Button
        android:id="@+id/buttonacido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="18dp"
        android:onClick="clickErro5"
        android:text="Ácido"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonErva"
        app:layout_constraintStart_toStartOf="@+id/buttonErva" />
</androidx.constraintlayout.widget.ConstraintLayout>